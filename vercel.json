{
  "redirects": [
    { "source": "/it", "destination": "/", "permanent": true },
    { "source": "/it/:path*", "destination": "/:path*", "permanent": true }
  ],
  "rewrites": [
    { "source": "/cv", "destination": "/cv.html" },
    { "source": "/portfolio", "destination": "/portfolio.html" },
    { "source": "/marketing", "destination": "/marketing.html" },
    { "source": "/promo", "destination": "/promo.html" },
    { "source": "/brand", "destination": "/brand.html" },
    { "source": "/Promo2", "destination": "/Promo2.html" },
    { "source": "/Promo1", "destination": "/Promo1.html" },
    { "source": "/portfolio1", "destination": "/portfolio1.html" },
    { "source": "/privacy", "destination": "/cookie-system.html" },
    { "source": "/cookies", "destination": "/cookie-system.html" },
    { "source": "/terms", "destination": "/cookie-system.html" },
    { "source": "/cookie-policy", "destination": "/cookie-system.html" },
    { "source": "/ebook", "destination": "/ebook.html" }

  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/cookie-system.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/brand.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}
